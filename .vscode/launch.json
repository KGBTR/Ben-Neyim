  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "localRoot": "${workspaceFolder}/app",
      "name": "Docker: Attach to Node",
      "cwd": "${workspaceFolder}/app",
      "preLaunchTask": "docker-run: debug",
      "remoteRoot": "/app"
    },
    {
      "type": "node",
      "request": "launch",
      "localRoot": "${workspaceFolder}/app",
      "name": "Docker: Launch to Node",
      "cwd": "${workspaceFolder}/app",
      "preLaunchTask": "docker-run: debug",
      "remoteRoot": "/app"
    },
  ]
}